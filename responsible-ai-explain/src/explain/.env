# Remote DB

RAI_EXPLAIN_DB = "${rai_explain_db}"
DB_NAME="${dbname}"
DB_USERNAME = "${username}"
DB_PWD = "${password}"
DB_IP = "${ipaddress}"
DB_PORT = "${port}"
DB_TYPE ="${dbtype}"
MONGO_PATH="mongodb://${DB_USERNAME}:${DB_PWD}@${DB_IP}:${DB_PORT}/"
COSMOS_PATH = "${cosmos_path}"

EXPLAINABILITY_TELEMETRY_URL = "${explainabilitytelemetryurl}"
