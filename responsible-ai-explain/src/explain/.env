DATASET_CONTAINER_NAME="${dataset_container_name}"
MODEL_CONTAINER_NAME="${model_container_name}"
PREPROCESSOR_CONTAINER_NAME="${preprocessor_container_name}"
HTML_CONTAINER_NAME="${html_container_name}"
TOKEN_IMP_URL = "${token_imp_url}"

